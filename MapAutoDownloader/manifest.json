{
  "manifest_version": 3,
  "name": "SimulASLAN Map Grabber",
  "version": "3.0",
  "permissions": [
    "downloads",
    "tabs",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/maps/d/u/0/mapimage*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}